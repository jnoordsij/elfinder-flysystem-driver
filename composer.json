{
    "name": "barryvdh/elfinder-flysystem-driver",
    "description": "A Flysystem Driver for elFinder",
    "license": "MIT",
    "keywords": ["elfinder", "flysystem", "filesystem"],
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "intervention/image": "^2.0",
        "league/flysystem": "^3.0",
        "league/glide": "^2.2",
        "studio-42/elfinder": "^2.1.58"
    },
    "suggest": {
        "league/glide": "2.x to display images through Glide",
        "spatie/flysystem-dropbox": "2.x to use Dropbox as filesystem"
    },
    "autoload": {
         "classmap": ["elFinderVolumeFlysystem.php"],
         "psr-4": {
            "Barryvdh\\elFinderFlysystemDriver\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
